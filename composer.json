{
    "name": "vyygir/front-end-renderer",
    "type": "library",
    "description": "Some classes to help you in creating static, front-end builds",
    "license": "Apache-2.0",
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {"FER\\": "src/FER/"}
    },
    "include-path": ["src/"]
}